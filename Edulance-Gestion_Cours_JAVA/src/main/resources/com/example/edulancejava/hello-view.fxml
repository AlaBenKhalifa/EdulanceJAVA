<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="626.0" prefWidth="1014.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="com.example.edulancejava.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <TableView prefHeight="200.0" prefWidth="800.0">
     <columns>
       <TableColumn fx:id="id" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="70.0" text="C1" />
       <TableColumn fx:id="name" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="70.0" text="C2" />
         <TableColumn fx:id="family_name" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="70.0" text="Column X" />
         <TableColumn fx:id="image" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="70.0" text="Column X" />
         <TableColumn fx:id="description" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="70.0" text="Column X" />
         <TableColumn fx:id="role" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="70.0" text="Column X" />
     </columns>
   </TableView>

  <Label fx:id="welcomeText" />
  <Button onAction="#onHelloButtonClick" text="Hello!" />
</VBox>